#include <iostream>

using namespace std;

int main() 
{ 
  double coffee_price = 60, discount, total;
  int number_of_cups = 5;
  bool if_tasty = true;
  discount = 13;
  coffee_price = coffee_price*(1-discount/100);
  total = number_of_cups*coffee_price;
  cout << "coffee_price = " << coffee_price << endl;
  cout << "total = " << total << endl;
  cout << "if_tasty = " << if_tasty << endl;
  return 0; 
}
